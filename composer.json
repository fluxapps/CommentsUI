{
  "name": "srag/commentsui",
  "description": "Comments ui",
  "version": "0.12.2",
  "extra": {
    "ilias_plugin": {
      "ilias_min_version": "5.4.0",
      "ilias_max_version": "6.999"
    },
    "generate_plugin_readme_template": "SRAG_ILIAS_LIBRARY",
    "long_description_template": "src/LONG_DESCRIPTION.md"
  },
  "type": "library",
  "keywords": [
    "comments",
    "ilias",
    "ui"
  ],
  "homepage": "https://github.com/fluxfw/CommentsUI",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "studer + raimann ag - Team Custom 1",
      "email": "support-custom1@studer-raimann.ch",
      "homepage": "https://studer-raimann.ch",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://plugins.studer-raimann.ch/goto.php?target=uihk_srsu_LCOMMENTSUI"
  },
  "require": {
    "php": ">=7.2",
    "srag/dic": ">=0.1.0",
    "srag/librarylanguageinstaller": ">=0.1.0"
  },
  "require-dev": {
    "srag/generateplugininfoshelper": ">=0.1.0"
  },
  "autoload": {
    "psr-4": {
      "srag\\CommentsUI\\": "src/"
    }
  },
  "config": {
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "generate-plugin-readme": [
      "srag\\GeneratePluginInfosHelper\\GeneratePluginReadme::generatePluginReadme"
    ]
  }
}